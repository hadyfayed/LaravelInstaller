  "name": "hadyfayed/laravel-installer",
  "description": "Laravel web installer",
  "license": "MIT",
  "authors": [
    {
      "name": "Rachid Laasri",
      "email": "rashidlaasri@gmail.com"
    },
    {
      "name": "Jeremy Kenedy",
      "email": "jeremykenedy@gmail.com"
    },
    {
      "name": "Hady Fayed",
      "email": "hadyfayed@gmail.com"
    }

  ],
  "require": {
    "php": ">=7.0.0"
  },
  "autoload": {
    "psr-4": {
      "HadyFayed\\LaravelInstaller\\": "src/"
    },
    "files": [
      "helpers/functions.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "HadyFayed\\LaravelInstaller\\Providers\\LaravelInstallerServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable"
}